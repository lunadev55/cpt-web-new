<script src="https://www.amcharts.com/lib/3/amcharts.js"></script>
<script src="https://www.amcharts.com/lib/3/serial.js"></script>
<script src="https://www.amcharts.com/lib/3/plugins/export/export.min.js"></script>
<link rel="stylesheet" href="https://www.amcharts.com/lib/3/plugins/export/export.css" type="text/css" media="all" />
<script src="https://www.amcharts.com/lib/3/themes/light.js"></script>
<style>
    #chartdiv {
	width	: 100%;
	height	: 90%;
	color: white !important;
}	
</style>
<div class="row">
  <div class="col-md-5 col-xs-2 row">
    <div class="col-md-1">
      De:
    </div>
    <div class="col-md-10 black">
      <%= date_field(:chart, :inicio, class: "input-group form-control", :value => (Time.now - 7.days).strftime('%Y-%m-%d')) %>
    </div>
  </div>
  <div class="col-md-5 col-xs-2">
    <div class="col-md-1">
      At√©:
    </div>
    <div class="col-md-10 black">
      <%= date_field(:chart, :fim, class: "input-group form-control", :value => Time.now.strftime('%Y-%m-%d')) %>
    </div>
   </div>
</div>
<div id="chartdiv" data-pair="<%= session[:currency1] %>_<%= session[:currency2] %>" class="row">
    
</div>
<%= javascript_include_tag 'views/trader/price_chart' %>
